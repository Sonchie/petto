<!-- Create New Booking -->

<div class="wrapper-new">
  <div class="new-pet container">
    <h4><%=@pet.name%></h4>
    <div class="row">
      <div class="col-sm-6 col-sm-offset-3">
        <%= render 'shared/view_pet_image' %>
        <h4>Select dates to book this Petto</h4>
        <%= render 'shared/booking_form' %>
      </div>
    </div>
  </div>
  <div class="redirect-all-pets">
   <%= link_to "Back to all pettos", pets_path, class: 'btn btn-warning' %>
  </div>
</div>

